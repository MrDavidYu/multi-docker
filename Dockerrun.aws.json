{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "davidyu2357/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "server",
			"image": "davidyu2357/multi-server",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "davidyu2357/multi-worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "davidyu2357/multi-nginx",
			"essential": true,
			"portMappings": [{"hostPort":80, "containerPort":80}],
			"links": ["client", "server"]
		}
	]
}